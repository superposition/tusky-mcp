{
  "name": "tusky-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Provider (MCP) server for the Tusky API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "tusky",
    "mcp",
    "model-context-provider",
    "sui",
    "walrus",
    "openapi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/mcp": "^0.9.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "node-cache": "^5.1.2",
    "openapi-parser": "^1.0.4",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "swagger-parser": "^10.0.3",
    "vectorize-text": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@types/swagger-parser": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}